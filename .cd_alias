#---------------------------------------------------------------------------------------

..() {
    #----------------------------#
    if [ "$1" = "" ];
    then
            cd ..;
        return;
    fi
    if [ "$1" = "d" ];
        then
            cd $DESKTOP;
        return;
    fi
    if [ "$1" = "g" ];
        then
            cd $GIT;
        return;
    fi
    #----------------------------#
    for ((x=0; $x<=$1; x++))
    do
        cd ..;
    done;
}

#---------------------------------------------------------------------------------------
alias la='ls -lA -G'
alias ls='ls -G'
